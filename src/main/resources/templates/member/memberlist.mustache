{{> /layout/header}}





<div class="container-xl p-5 ">
    <div class="p-5 mt-3 ">
        <label class="form-label">포지션 </label>
        <select class="form-select" id="position" name="position">
            <option>내야수</option>
            <option>외야수</option>
            <option>타자</option>
            <option>투수</option>
        </select>
    </div>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>번호</th>
                <th>팀 이름</th>
                <th>포지션</th>
                <th>선수 이름</th>
                <th>등록일</th>
                <th>수정</th>
                <th>삭제</th>
            </tr>
        </thead>
        <tbody>
            {{#member}}
            <tr>
                <td>{{id}}</td>
                <td>{{teamName}}</td>
                <td>{{position}}</td>
                <td>{{memberName}}</td>
                <td>{{date}}</td>
                <td>수정하기</td>
                <td>삭제하기</td>
            </tr>
            {{/member}}
        </tbody>
    </table>
</div>


{{> /layout/footer}}