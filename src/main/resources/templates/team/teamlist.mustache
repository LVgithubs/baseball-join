            <tr>
                <th>번호</th>
                <th>경기장 이름</th>
                <th>팀 이름</th>
                <th>창단일</th>
                <th>수정</th>
                <th>삭제</th>
            </tr>
        </thead>
        <tbody>
            {{#team}}
            <tr>
                <td>{{id}}</td>
